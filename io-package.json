    "license": "MIT",
    "platform": "Javascript/Node.js",
    "main": "build/main.js",
    "icon": "dingz.png",
    "enabled": true,
    "extIcon": "https://raw.githubusercontent.com/rgwch/ioBroker.dingz/master/admin/dingz.png",
    "readme": "https://github.com/rgwch/ioBroker.dingz/blob/master/README.md",
    "loglevel": "info",
    "restartAdapters": [
      "vis"
    ],
    "localLink": "%web_protocol%://%ip%:%web_port%/vis/edit.html",
    "mode": "daemon",
    "type": "lighting",
    "compact": true,
    "connectionType": "local",
    "dataSource": "poll",
    "materialize": true,
    "materializeTab": true,
    "adminTab": {
      "singleton": true,
      "name": {
        "en": "Dingz",
        "de": "Dingz",
        "ru": "Dingz",
        "pt": "Dingz",
        "nl": "Dingz",
        "fr": "Dingz",
        "it": "Dingz",
        "es": "Dingz",
        "pl": "Dingz",
        "zh-cn": "丁茨"
      },
      "link": "",
      "fa-icon": "info"
    },
    "supportCustoms": true,
    "dependencies": [
      {
        "js-controller": ">=1.4.2"
      },
      "vis"
    ]
  },
  "native": {
    "interval": "60"
  },
  "objects": [],
  "instanceObjects": [
    {
      "_id": "info",
      "type": "channel",
      "common": {
        "name": "Information"
      },
      "native": {}
    },
    {
      "_id": "info.connection",
      "type": "state",
      "common": {
        "role": "indicator.connected",
        "name": "Device or service connected",
        "type": "boolean",
        "read": true,
        "write": false,
        "def": false
      },
      "native": {}
    },
    {
      "_id": "info:deviceInfo",
      "type": "channel",
      "common": {
        "name": "Hardware Informationen"
      },
      "native": {}
    },
    {
      "_id": "info.deviceInfo.mac",
      "type": "state",
      "common": {
        "role": "state",
        "name": "mac",
        "type": "string",
        "read": true,
        "write": false
      },
      "native": {}
    },
    {
      "_id": "info.deviceInfo.details",
      "type": "state",
      "common": {
        "role": "json",
        "type": "string",
        "name": "Detailed informations",
        "read": true,
        "write": false
      },
      "native": {}
    }
  ]
}